%format (N.Table (t) (x) (c) (k)) = " \textbf{table}(n) "
%format (N.App (t) (e1) (e2)) = e1 "~" e2
%format (N.AppE1 (t) (p) (e)) = p "~" e
%format (N.AppE2 (t) (p) (e1) (e2)) = p "~" e1 "~" e2
%format (N.BinOp (t) (o) (e1) (e2)) = e1 "~" o "~" e2
%format (N.Lam (t) (x) (e)) = " \lambda " x "~\rightarrow~" e
%format (N.If (t) (e1) (e2) (e3)) = " \textbf{if}~" e1 "~\textbf{then}~" e2 " ~\textbf{else}~ " e3 
%format (N.Const (t) (c)) = c
%format (N.Var (t) (s)) = s
%format String = " x " 
